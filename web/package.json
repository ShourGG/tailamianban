{
  "name": "robot-admin",
  "version": "1.1.9.25",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "prebuild": "node scripts/sync-version.js",
    "postinstall": "simple-git-hooks",
    "dev": "env-manager dev && vite",
    "build": "env-manager prod && vite build",
    "build:dev": "env-manager dev && vite build --mode development",
    "build:staging": "env-manager staging && vite build --mode staging",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@antv/g6": "^5.0.28",
    "@antv/s2": "^2.0.0-next.18",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/vue-next": "^1.21.0",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.7",
    "bytemd": "^1.21.0",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.13",
    "driver.js": "^1.3.1",
    "echarts": "^5.5.1",
    "jszip": "^3.10.1",
    "naive-ui": "^2.40.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "uuid": "^10.0.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue3-calendar-heatmap": "^2.0.5",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@iconify/json": "^2.2.264",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@types/uuid": "^10.0.0",
    "@unocss/eslint-plugin": "^0.63.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.6.3",
    "unocss": "^0.63.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,md}": [
      "prettier --write"
    ]
  }
}
